{
    "name": "qubus/inheritance",
    "description": "A library of traits that can be used in place of helper classes or god classes.",
    "type": "library",
    "keywords": ["traits", "multiple inheritance", "inheritance"],
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Parker",
            "email": "josh@joshuaparker.blog"
        }
    ],
    "support": {
        "issues": "https://github.com/QubusPHP/inheritance/issues"
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "^2.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Qubus\\Inheritance\\": ""
        }
    },
    "scripts": {
        "cs-check": "phpcs",
        "cs-fix": "phpcbf"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
